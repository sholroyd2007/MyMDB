@model MovieSeries

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>MovieSeries</h4>
    <hr />
    @Html.EditorForModel()
    <ul>
        @foreach (var item in Model.Movies)
       {
           <li>@item.Name</li>
       }
    </ul>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
